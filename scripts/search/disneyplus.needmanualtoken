from requests import post
import sys

email = sys.argv[1]

def disneyplus(email):
  try:
    head = {"User-Agent":"user-agent: BAMSDK/v4.18.0 (disney-svod-3d9324fc 1.11.2.0; v2.0/v4.18.0; android; phone) samsung SM-J9 (LMY47X.J200BTUBU2AQB2; Linux; 10; API 26)"}
    head["authorization"] = auth
    dt = {"email":email}
    res = post(url="https://global.edge.bamgrid.com/idp/check",headers=head,json=dt).text
    print(res)
    if not "Register" in res:
      print("Found")
    else:
      return False
  except:
    None

disneyplus(email)
